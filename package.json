{
    "name": "lines-time",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel --no-daemon",
        "lint": "rome check .",
        "preview": "turbo run preview --parallel --no-daemon",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "eslint-config-custom": "*",
        "prettier": "latest",
        "rome": "^11.0.0",
        "turbo": "latest"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "packageManager": "yarn@3.3.0"
}
